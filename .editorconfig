# Add plugin for your editors
# see: http://editorconfig.org

root = true

[*]
end_of_line = lf
charset = utf-8
tab_width = 8

[*.{js,md,java}]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
insert_final_newline = true

[*.json]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
insert_final_newline = true
